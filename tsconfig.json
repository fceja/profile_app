{
  "compilerOptions": {
    "baseUrl": "./",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react",
    "module": "CommonJS",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "outDir": "./dist",
    "paths": {
      "@/*": ["./src/*"],
      "@assets/*": ["./src/assets/*"],
      "@components/*": ["./src/ts/components/*"],
      "@common/*": ["./src/ts/common/*"],
      "@data/*": ["src/ts/data/*"],
      "@pages/*": ["./src/ts/pages/*"],
      "@scripts/*": ["./src/ts/utils/scripts/*"],
      "@store/*": ["./src/ts/store/*"],
      "@scss/*": ["./src/scss/*"],
      "@utils/*": ["./src/ts/utils/*"]
    },
    "skipLibCheck": true,
    "sourceMap": false,
    "strict": true,
    "strictNullChecks": true,
    "target": "es2016"
  }
}
